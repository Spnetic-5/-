{
  "expo": {
    "name": "Expense Tracker",
    "slug": "Expense_Tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/apla  .png",
    "splash": {
      "image": "./assets/apla.png",
      "resizeMode": "contain",
      "backgroundColor": "#402243"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/apla.png",
        "backgroundColor": "#000000"
      }
    }
  }
}